{
  "author": "Leon McCarthy",
  "name": "Letsfakenews",
  "website": "https://factoryofthought.com/#letsfakenews",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint '**/*.js' --fix",
    "lint_folders": "eslint 'models/*.js' 'modules/*.js' 'routes/*.js' 'test/modules/*.js' 'app.js' --fix",
    "heroku-postbuild": "cd client && npm install && GENERATE_SOURCEMAP=false npm run build",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test_react": "cd client && npm run test",
    "test_coverage_text": "nyc --reporter=text mocha ./test --recursive",
    "test_coverage_html": "nyc --reporter=html mocha ./test --recursive",
    "test_snyk": "snyk test && mocha ./test --recursive",
    "test": "mocha ./test --recursive",
    "test_nlp": "mocha ./test/modules/nlp.test.js",
    "test_imagesearch": "mocha ./test/modules/imagesearch.test.js",
    "test_process": "mocha ./test/modules/process.test.js",
    "test_time_ops": "mocha ./test/modules/timeOps.test.js",
    "test_db_fetch_mode": "mocha ./test/modules/dbFetchMode.test.js",
    "test_refresh": "mocha ./test/modules/refresh.test.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.4.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "events": "^3.0.0",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "google-images": "^2.1.0",
    "helmet": "^3.21.3",
    "http-auth": "^4.1.2",
    "ismobilejs": "^1.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.9.1",
    "node": "13.7.0",
    "npm": "^6.14.6",
    "popper.js": "^1.16.1",
    "pos": "^0.4.2",
    "s": "^1.0.0",
    "serve-favicon": "^2.5.0",
    "to-boolean": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^5.0.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "jquery": "^3.5.0",
    "nyc": "^15.0.0",
    "snazzy": "^8.0.0",
    "snyk": "^1.298.1"
  }
}
