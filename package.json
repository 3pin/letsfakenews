{
  "author": "Leon McCarthy",
  "name": "Letsfakenews",
  "website": "https://factoryofthought.com/#letsfakenews",
  "license": "GPL-3.0",
  "version": "0.0.1",
  "engines": {
    "node": "10.x"
  },
  "private": true,
  "scripts": {
    "lint": "eslint '**/*.js' --fix",
    "lint_folders": "eslint 'models/*.js' 'modules/*.js' 'routes/*.js' 'test/modules/*.js' 'app.js' --fix",
    "heroku-postbuild": "cd client && npm install && GENERATE_SOURCEMAP=false npm run build",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test_react": "cd client && npm run test",
    "test_coverage_text": "nyc --reporter=text mocha ./test --recursive",
    "test_coverage_html": "nyc --reporter=html mocha ./test --recursive",
    "test_no_snyk": "mocha ./test --recursive",
    "test": "snyk test && mocha ./test --recursive",
    "test_nlp": "mocha ./test/modules/nlp.test.js",
    "test_imagesearch": "mocha ./test/modules/imagesearch.test.js",
    "test_process": "mocha ./test/modules/process.test.js",
    "test_time_ops": "mocha ./test/modules/timeOps.test.js",
    "test_db_fetch_mode": "mocha ./test/modules/dbFetchMode.test.js",
    "test_refresh": "mocha ./test/modules/refresh.test.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "events": "^3.3.0",
    "express": "^4.17.1",
    "express-device": "^0.4.2",
    "google-images": "^2.1.0",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "pos": "^0.4.2",
    "serve-favicon": "^2.5.0",
    "to-boolean": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "concurrently": "^6.0.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "snyk": "^1.529.0"
  }
}
